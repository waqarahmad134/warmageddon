{

    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "anhskohbo/no-captcha": "^3.1",
        "automattic/woocommerce": "^3.0",
        "blocktrail/cryptojs-aes-php": "~0.1.0",
        "brian2694/laravel-toastr": "^5.5",
        "chencha/share": "^5.2",
        "coingate/coingate-php": "^3.0",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.2",
        "florianv/laravel-swap": "^2.3",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "^2.4",
        "itsgoingd/clockwork": "^5.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^3.2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "league/csv": "^9.7",
        "nyholm/psr7": "^1.4",
        "php-http/curl-client": "^2.2",
        "php-http/message": "^1.11",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "sendgrid/sendgrid": "~7",
        "spatie/db-dumper": "^2.17",
        "spatie/laravel-activitylog": "3.9",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-newsletter": "^4.8",
        "spatie/laravel-permission": "^2.37",
        "stevebauman/location": "^5.2",
        "stripe/stripe-php": "^7.49",
        "symfony/yaml": "^5.2",
        "twilio/sdk": "^5.35",
        "tymon/jwt-auth": "1.0.0",
        "wladmonax/laravel-db-backup": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [

            "app/Helper/Helper.php"

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
